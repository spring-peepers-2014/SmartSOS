<%= form_tag sessions_create_organizations_path do %>

	<p>
  <%= label_tag :email %> <br>
  <%= text_field_tag :email, params[:email] %>
  </p>

  <p>
  <%= label_tag :password %> <br>
	<%= password_field_tag :password, params[:password] %>
	</p>

  <%= submit_tag(value = "Organization Login") %>
<% end %>
